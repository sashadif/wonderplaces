!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}
    %title WonderPlaces - Home
    %meta{:charset => "utf-8"}
    %meta{:content => "width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no", :name => "viewport"}
    //%link{:href => "img/favicon.ico", :rel => "shortcut icon", :type => "image/x-icon"}
    = stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    %script{:src => "https://maps.googleapis.com/maps/api/js?key=AIzaSyA8Ul21wMPLeI8q8MwwJ7f75JFHTZ6oeEU&language=en", :type => "text/javascript"}
    //%script{:src =>"https://code.jquery.com/ui/1.10.2/jquery-ui.js"}
    //%link{:href => "https://code.jquery.com/ui/1.10.2/themes/smoothness/jquery-ui.css", :rel =>"Stylesheet"}
    = csrf_meta_tags
  %body{:class => "#{controller_name} #{action_name}"}
    = render "layouts/header"
    %main#main-content
      = yield
    = render "layouts/footer"


